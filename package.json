{
  "name": "mevn-cli",
  "version": "1.8.0-beta.0",
  "description": "Light Speed setup for MEVN stack based apps.",
  "main": "./bin/mevn.js",
  "files": [
    "bin",
    "lib"
  ],
  "bin": {
    "mevn": "./bin/mevn.js"
  },
  "scripts": {
    "build": "gulp clean && babel src --out-dir lib --copy-files && gulp copy",
    "build:watch": "babel --watch src --out-dir lib --copy-files",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "pretest": "npm run lint",
    "test": "jest",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "postinstall": "opencollective-postinstall || true"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madlabsinc/mevn-cli.git"
  },
  "keywords": [
    "node.js",
    "commander.js",
    "inquirer.js",
    "shell.js",
    "cli",
    "mevn",
    "vue",
    "express",
    "mongodb"
  ],
  "author": "James George <jamesgeorge998001@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/madlabsinc/mevn-cli/issues"
  },
  "homepage": "https://github.com/madlabsinc/mevn-cli#readme",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "chalk": "^2.4.1",
    "commander": "^2.16.0",
    "didyoumean": "^1.2.1",
    "envinfo": "^7.3.1",
    "execa": "^1.0.0",
    "inquirer": "^6.0.0",
    "node-banner": "^1.3.2",
    "opencollective-postinstall": "^2.0.2",
    "ora": "^3.4.0",
    "update-notifier": "^3.0.0",
    "validate-npm-package-name": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "del": "^4.1.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-prettier": "3.0.1",
    "gulp": "^4.0.2",
    "husky": "^1.3.1",
    "jest": "^24.8.0",
    "lint-staged": "^8.2.1",
    "prettier": "^2.0.4",
    "vuepress": "^0.14.11"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/mevn-cli"
  }
}
