{
  "node": true, 
  "browser": true, 
  "esversion": 6, 
  "bitwise": false, 
  "camelcase": false, 
  "curly": false, 
  "eqeqeq": true, 
  "immed": true, 
  "newcap": true, 
  "noarg": true, 
  "quotmark": "single", 
  "regexp": true, 
  "undef": true, 
  "unused": true, 
  "strict": true, 
  "trailing": true, 
  "smarttabs": false,
  "predef": [ 
    "define",
    "require",
    "exports",
    "module",
    "describe",
    "before",
    "beforeEach",
    "after",
    "afterEach",
    "it",
    "inject",
    "expect",
    "spyOn"
  ],
  "indent": 2, // Specify indentation spacing
  "devel": false, // Allow development statements e.g. `console.log();`.
  "noempty": true, // Prohibit use of empty blocks.
  "latedef": "nofunc"
}